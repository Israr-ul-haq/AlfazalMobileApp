{
  "expo": {
    "name": "alfazal.com",
    "slug": "BakeryApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.israr02.BakeryApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["INTERNET"],
      "package": "com.israr02.BakeryApp"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "scheme": "alfazal.com",
    "extra": {
      "eas": {
        "projectId": "470bff32-608d-401c-be0b-8844ff1cbb7c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/470bff32-608d-401c-be0b-8844ff1cbb7c"
    }
  }
}
